<div class="nested-fields">
  <div class="field">
    <%= f.label :day_of_week %>
    <%= f.select :day_of_week, Schedule.day_of_weeks.keys %>
  </div>
  <div class="field">
    <%= f.label :session %>
    <%= f.select :session, Schedule.sessions.keys %>
  </div>
  <div class="field">
    <%= f.label :start_time %>
    <%= f.time_select :start_time, { minute_step: 15 } %>
  </div>
  <div class="field">
    <%= f.label :end_time %>
    <%= f.time_select :end_time, { minute_step: 15 } %>
  </div>
  

  <div class="field">
    <%= link_to 'Remove', '#', class: 'remove_fields', data: { turbo_action: 'click->clinicForm#removeFields' } %>
  </div>

</div>

<div class="py-3">
  <hr />
</div>